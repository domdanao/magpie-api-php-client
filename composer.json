{
    "name": "domdanao/magpie-api-php-client",
    "description": "",
    "keywords": [
        "domdanao",
        "magpie-api-php-client"
    ],
    "homepage": "https://github.com/domdanao/magpie-api-php-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dominick Danao",
            "email": "dom@danao.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Domdanao\\MagpieApiPhpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Domdanao\\MagpieApiPhpClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
